# 前端环境变量配置
VITE_API_BASE_URL=https://unlimited-api.xzt.plus
VITE_CLIENT_PORT=3001
VITE_APP_NAME=无限域
VITE_APP_VERSION=1.0.0

# 开发环境配置
VITE_NODE_ENV=production
VITE_DEBUG=true

# API配置
VITE_API_TIMEOUT=10000
VITE_API_VERSION=v1

# 路由配置
# 可选值: 'hash' 或 'browser'
# hash: 使用hash路由模式，适合GitHub Pages等静态托管
# browser: 使用浏览器路由模式，需要服务器支持
VITE_ROUTER_MODE=hash